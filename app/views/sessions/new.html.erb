







<nav> 
 
  <ul class="nav navbar-nav navbar-right">
    <li><a class="btn btn-outline-light text-primary" href="sign_up"><span class="glyphicon glyphicon-user"></span> </a></li>
  </ul>
</nav>
<div class="center posi size text-primary">
  <h3 class="card-subtitle mb-3 center">Sign In</h3> 
    <%= form_with url: login_path do |form| %>
      <div class="mb-3">
        <%= form.label :email %>
        <%= form.text_field :email, class: "form-control" %> 
      </div>
      <div class="mb-3 ">
        <%= form.label :password %>
        <%= form.password_field :password, class: "form-control" %>
        </div>
      
      <div class="mb-3">
        <%= form.submit "Sign in" , class: "btn btn-primary center" %>
      </div>
   
      
    <% end %>
</div>


